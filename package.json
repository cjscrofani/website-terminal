{
  "name": "react-terminal-search",
  "version": "1.0.0",
  "description": "A retro terminal-style search interface for React/Next.js with Unix-like commands",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "nextjs",
    "terminal",
    "search",
    "command-line",
    "cli",
    "retro",
    "ui-component",
    "navigation",
    "unix"
  ],
  "author": "CJ Scrofani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cjscrofani/react-terminal-search"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "tailwindcss": "^3.0.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "lucide-react": "^0.400.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "jsdom": "^23.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.4"
  },
  "scripts": {
    "build": "tsc && npm run copy-styles",
    "copy-styles": "mkdir -p dist/styles && cp src/styles/terminal.css dist/styles/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  }
}
